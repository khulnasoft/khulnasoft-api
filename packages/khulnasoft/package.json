{
  "name": "khulnasoft",
  "version": "0.0.1",
  "description": "KhulnaSoft api server and client",
  "author": "dev@khulnasoft.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/khulnasoft-api.git",
    "directory": "packages/khulnasoft"
  },
  "homepage": "https://github.com/khulnasoft/khulnasoft-api/tree/main/packages/khulnasoft",
  "bugs": {
    "url": "https://github.com/khulnasoft/khulnasoft-api/issues"
  },
  "keywords": [
    "khulnasoft",
    "api"
  ],
  "license": "ISC",
  "source": "src/khulnasoft.ts",
  "bin": {
    "gen-khulnasoft-api-route-map": "./dist/cli/gen-khulnasoft-api-route-map.js"
  },
  "main": "dist/khulnasoft.js",
  "types": "dist/khulnasoft.d.ts",
  "exports": {
    ".": {
      "types": "./dist/khulnasoft.d.ts",
      "default": "./dist/khulnasoft.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "prebuild": "mkdir -p dist/esm && echo '{\"type\": \"module\"}' > dist/esm/package.json",
    "test": "pnpm -r test",
    "test:ci": "start-server-and-test start-test-server http://0.0.0.0:3005 test",
    "build": "pnpm -r run prebuild && tsc -b packages/*/tsconfig*.json && (cd demo && pnpm build)"
  },
  "devDependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.3",
    "@types/qs": "^6.9.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "openapi3-ts": "^4.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "dedent-js": "^1.0.1",
    "lodash": "^4.17.21",
    "qs": "^6.11.2",
    "ts-node": "^10.9.1",
    "zod": "^3.22.4",
    "zod-openapi": "^5.4.5",
    "zod-validation-error": "^4.0.0"
  }
}
